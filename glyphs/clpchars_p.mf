% numbers

clpchar("zero",5u#,ht#,0);
pickup cornerpen;
x1=0.9w;
x2=x4=0.5w;
x3=0.1w;
top y2=h+o;
y1=y3=0.5h;
bot y4=-o;
interdotdraw z4{right}..z1{up}..z2{left}..z3{down}..cycle;
adjustwidth(xgap#,xgap#);
penlabels(range 1 thru 150);
endchar;

clpchar("one",3.5u#,ht#,0);
pickup cornerpen;
x1=x2=0.7w;
x3=x5=0.1w;
bot y1=0;
top y2=h;
y3=0.7h;
dotdraw z1--z2--z3;
adjustwidth(xgap#,xgap#);
penlabels(range 1 thru 150);
endchar;

clpchar("two",5u#,ht#,0);
