clpchar("ampersand",5u#,ht#,0);
pickup cornerpen;
x1=0;
x7=0.2w;
x5=0.35w;
x4=0.5w;
x3=0.5w;
x8=w;
x9=w-0.5px;
x6=0.8w;
bot y4=0-o;
bot y9=0;
top y3=h+o;
y6=y7=0.8h+0.5o;
y5=0.6h;
y1=y8=0.3h+0.5o;
dy=angle(z9-z5);
%z5=whatever[z1,z2]=whatever[z4,z6];
dotdraw z9--z5{-dir dy}..z7{up}..z3{right}..z6{down}..z5..z1{down}..z4{right}..z8;
adjustwidth(xgap#,xgap#);
penlabels(range 1 thru 150);
endchar;

