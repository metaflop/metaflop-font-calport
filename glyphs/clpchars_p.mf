clpchar("ampersand",5.5u#,ht#,0);
pickup cornerpen;
x1=0.05w;
x7=0.2w;
x5=0.4w;
x4=0.4w;
x3=0.475w;
x8=0.9w;
x9=0.95w;
x6=0.75w;
bot y4=0-o;
bot y9=0;
top y3=h+o;
y6=y7=0.8h+0.5o;
y5=0.55h;
y1=0.3h+0.5o;
y8=0.45h;
z10=(z4{right}..z8) intersectionpoint (z5--z9);
path p;
p:=z9--z10--z5{z5-z9}..z7{up}..z3{right}..z6{down}..z5..z1{down}..z4{right}..z10..z8;
interdotdraw subpath(0,1) of p;
interdotdraw subpath(1,2) of p;
interdotdraw subpath(2,6) of p;
interdotdraw subpath(6,9) of p;
interdotdraw subpath(9,10) of p;
adjustwidth(xgap#,xgap#);
penlabels(range 1 thru 150);
endchar;

