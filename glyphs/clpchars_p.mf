% grave
if (sset=1) or (sset=2): 
	clpchar("grave",2.5u#,x_ht#+acc_ht#,0); 
	pickup cornerpen;
	x1=0.1w;
	x2=0.95w;
	top y1=h;
	y2=.8h;
	dotdraw z1--z2;
	charanchortops_[charcode]=(.5w,x_ht);
	adjustwidth(xgap#,xgap#);
	penlabels(range 1 thru 150);
	endchar;
elseif sset=3:
	clpchar("grave",4.5u#,x_ht#+acc_ht#,0); 
	pickup cornerpen;
	x1=0.29w;
	x2=0.76w;
	top y1=h;
	y2=.8h;
	dotdraw z1--z2;
	charanchortops_[charcode]=(.5w,x_ht);
	adjustwidth(xgap#,xgap#);
	penlabels(range 1 thru 150);
	endchar;
fi

